{
    "name": "silvercommerce/checkout",
    "description": "Adds a 'checkout' to a SilverStripe site which links to an Estimate and adds the ability to setup and pay",
      "type": "silverstripe-vendormodule",
      "homepage": "http://github.com/silvercommerce/checkout",
      "keywords": [
          "silverstripe",
          "commerce",
          "ecommerce",
          "checkout",
          "payments"
      ],
      "license": "BSD-3-Clause",
      "authors": [
          {
              "name": "Morven Lewis-Everley",
              "email": "morven@ilateral.co.uk"
          },
          {
              "name": "Mark Anderson",
              "email": "mark@ilateral.co.uk"
          }
      ],
      "support": {
          "issues": "http://github.com/silvercommerce/checkout/issues"
      },
      "require": {
          "silverstripe/framework": "~4.0",
          "silverstripe/silverstripe-omnipay": "~3.0",
          "silvercommerce/settings": "~1.0",
          "silvercommerce/orders-admin": "~1.0",
          "silvercommerce/contact-admin": "~1.0",
          "silvercommerce/postage": "~1.0"
      },
      "suggest": {
          "silverstripe/cms": "Add full content management support to your site",
          "i-lateral/silverstripe-users": "Add user account management and registration"
      },
      "extra": {
          "expose": [
              "client/dist"
          ]
      },
      "scripts": {
          "lint": "phpcs -s src/ tests/",
          "lint-clean": "phpcbf src/ tests/"
      },
      "autoload": {
          "psr-4": {
              "SilverCommerce\\Checkout\\": "src/"
          }
      }
  }
